{
    "default": true,
    "MD013": {
        "line_length": 120,
        "headings": false,
        "code_blocks": false
    },
    "MD033": {
        "allowed_elements": [
            "mermaid",
            "br",
            "details",
            "summary"
        ]
    },
    "MD048": {
        "style": "backtick"
    },
    "MD024": {
        "allow_different_nesting": true,
        "siblings_only": true
    },
    "MD026": {
        "punctuation": ".,;:!"
    },
    "MD029": {
        "style": "ordered"
    },
    "MD046": {
        "style": "consistent"
    },
    "MD025": {
        "front_matter_title": ""
    },
    "MD040": true,
    "MD004": {
        "style": "dash"
    }
}
